    "@tiptap/extension-link": "2.4.0",
    "@tiptap/extension-text-align": "2.4.0",
    "@tiptap/pm": "2.4.0",
    "@tiptap/react": "2.4.0",
    "@tiptap/starter-kit": "2.4.0",
    "@types/passport-google-oauth20": "2.0.16",
    "@uploadthing/react": "6.6.0",
    "blitz": "2.0.10",
    "date-fns": "3.6.0",
    "dayjs": "1.11.11",
    "eslint": "9.7.0",
    "next": "14.2.4",
    "nodemailer": "6.9.14",
    "passport": "0.7.0",
    "passport-google-oauth20": "2.0.0",
    "prisma": "5.15.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
