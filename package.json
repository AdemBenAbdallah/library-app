    "@testing-library/react": "16.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "20.14.8",
    "@types/preview-email": "3.0.4",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@vitejs/plugin-react": "4.3.1",
    "ajv": "^8.16.0",
    "enforce-branch-name": "1.1.2",
    "eslint-config-next": "14.2.4",
